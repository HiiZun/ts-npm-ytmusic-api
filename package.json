{
	"name": "@hiizun/ytmusic-api",
	"version": "5.3.0",
	"description": "YouTube Music API",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"author": "zS1L3NT <dev@zectan.com> (http://www.zectan.com)",
	"license": "GPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/HiiZun/ts-npm-ytmusic-api"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"files": [
		"dist",
		"package.json"
	],
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts",
		"lint": "tsc --noEmit && eslint src --ext .ts --fix && prettier --write \"src/**/*.ts\"",
		"lint:check": "tsc --noEmit && eslint src --ext .ts && prettier --check \"src/**/*.ts\"",
		"format": "prettier --write \"src/**/*.ts\"",
		"test": "bun test || echo 'No tests specified'",
		"prepublishOnly": "npm run build",
		"clean": "rimraf dist"
	},
	"dependencies": {
		"axios": "^1.7.2",
		"https-proxy-agent": "^7.0.6",
		"socks-proxy-agent": "^8.0.5",
		"tough-cookie": "^4.1.4",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/node": "^24.0.15",
		"@types/tough-cookie": "^4.0.5",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"bun-types": "^1.1.18",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"prettier": "^3.2.5",
		"tsup": "^8.1.0",
		"typescript": "5.1",
		"zod-to-json-schema": "^3.23.1"
	},
	"keywords": [
		"youtube",
		"music",
		"api",
		"proxy",
		"http-proxy",
		"socks-proxy",
		"scraper"
	]
}
